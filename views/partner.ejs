            <script>
                function initMap() {
                    var newDelhi = { lat: 28.6139, lng: 77.2090 };
                    var map = new google.maps.Map(document.getElementById('map'), {
                        zoom: 12,
                        center: newDelhi
                    });
                    var marker = new google.maps.Marker({
                        position: newDelhi,
                        map: map,
                        title: 'New Delhi'
                    });
                }
            </script>

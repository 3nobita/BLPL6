        <body>
            <div class="container">

                <section class="section2">
                    <div class="col2 first">
                        <div id="map" style="height: 600px; width: 100%;"></div>
                    </div>
                    <div class="col2 last">
                        <div class="sec2innercont">
                            <div class="sec2addr">
                                <p><i class="fas fa-university"></i> E-3, Top Floor, Tower A, Bhavishya
                                    Apartment, Nai Basti Road, Deoli, New Delhi -110062</p>
                                <p><i class="fas fa-phone"></i> <span class="collig">Phone :</span>+91 88510
                                    41909</p>
                                <p><i class="fas fa-envelope"></i> <span class="collig">Email :</span>
                                    info@Blplfinserv.in</p>
                                <p><i class="fas fa-fax"></i> <span class="collig">Fax :</span>+91 88510
                                    41909
                                </p>
                            </div>

                        </div>
                        <div class="sec2contactform">
                            <h3 style="color: #103C63;" class="sec2frmtitle">Want to Know More?? Drop Us a
                                Mail</h3>
                            <form action="/clients">
                                <div class="clearfix">
                                    <input type="text" name="name" placeholder="Your Name*" class="form-control"
                                        required>
                                </div>
                                <div class="clearfix">
                                    <input type="text" placeholder="Phone Number*" id="mobileNo" name="mobileNo"
                                        class="form-control" required>
                                </div>
                                <div class="clearfix">
                                    <textarea class="form-control" name="message"
                                        id="message">Your message here...</textarea>
                                </div>
                                <div class="clearfix">
                                    <input type="text" id="otp" name="otp" class="form-control" placeholder="Enter OTP*"
                                        required>
                                </div>
                                <button style="margin-bottom: 2px;" type="button" onclick="sendOtp()">Send
                                    OTP</button>
                                <button type="submit">Submit</button>
                            </form>
                            <script>
                                async function sendOtp() {
                                    const mobileNo = document.getElementById('mobileNo').value;

                                    if (!mobileNo) {
                                        alert('Please enter a phone number.');
                                        return;
                                    }

                                    try {
                                        const response = await fetch('/send-otp', {
                                            method: 'POST',
                                            headers: { 'Content-Type': 'application/json' },
                                            body: JSON.stringify({ mobileNo })
                                        });

                                        const data = await response.json();

                                        if (data.success) {
                                            alert('OTP sent successfully!');
                                        } else {
                                            alert('Failed to send OTP.');
                                        }
                                    } catch (error) {
                                        console.error('Error:', error);
                                        alert('An error occurred.');
                                    }
                                }
                            </script>
                        </div>
                    </div>
                </section>
